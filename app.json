{
  "expo": {
    "name": "BumpBites",
    "slug": "BumpBites",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Avocado.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Avocado.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chrismega.BumpBites"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Avocado.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Avocado.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8f627d9d-9ee8-4c53-99e4-f31966409f84"
      }
    },
    "owner": "chris-mega"
  }
}
